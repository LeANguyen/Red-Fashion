lassName=\"navbar-brand\">\n          <h1 className=\"m-0\">\n            <Link className={`${HeaderCss[\"logo\"]}`} to=\"/\">\n              Fashion\n            </Link>\n          </h1>\n        </div>\n\n        {/* header body */}\n        <div className=\"collapse navbar-collapse\" id=\"navbarToggler\">\n          <ul className=\"navbar-nav mr-auto\">\n            {/* home */}\n            <li className=\"nav-item mx-lg-2\">\n              <Link className=\"nav-link\" to=\"/\">\n                <h5 className={HeaderCss[\"link\"]}>\n                  <i className=\"fa fa-home\"></i>\n                  <Space></Space>\n                  <Space></Space>Home\n                </h5>\n              </Link>\n            </li>\n\n            {/* shop categories */}\n            <li className=\"nav-item dropdown mx-lg-2\">\n              <Dropdown\n                _iconName=\"black-tie\"\n                _text=\"Categories\"\n                _displayToggle\n                _textBold\n                _className=\"btn-outline-pink\"\n              >\n                <Link className=\"dropdown-item\" to=\"/items/Shirt\">\n                  <i className=\"fa fa-star\"></i>\n                  <Space></Space>\n                  <Space></Space>\n                  Shirt\n                </Link>\n                <Link className=\"dropdown-item\" to=\"/items/Pant\">\n                  <i className=\"fa fa-star\"></i>\n                  <Space></Space>\n                  <Space></Space>\n                  Pant\n                </Link>\n                <Link className=\"dropdown-item\" to=\"/items/Shoe\">\n                  <i className=\"fa fa-star\"></i>\n                  <Space></Space>\n                  <Space></Space>\n                  Shoe\n                </Link>\n              </Dropdown>\n            </li>\n\n            {/* login - when user not logged in */}\n            {user === null && (\n              <li className=\"nav-item mx-lg-2\">\n                <a\n       