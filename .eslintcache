ute>\n            <Route exact path=\"/admin\" component={AdminPage}></Route>\n            <Route\n              exact\n              path=\"/items/:category\"\n              component={ItemListPage}\n            ></Route>\n            <Route\n              exact\n              path=\"/item_detail/:id\"\n              component={ItemDetailPage}\n            ></Route>\n            <Route exact path=\"/cart/:id?\" component={CartPage}></Route>\n            <Route exact path=\"/admin\" component={AdminPage}></Route>\n            <Route exact path=\"/test\" component={TestPage}></Route>\n            <Route component={NotFoundPage} />\n          </Switch>\n        </Router>\n      )}\n    </>\n  );\n}\n\nexport default App;\n","/Users/mac/Desktop/React Projects/Red-Fashion/src/reportWebVitals.js",[],"/Users/mac/Desktop/React Projects/Red-Fashion/src/hooks/useApi.js",["306"],"import React, { useState } from \"react\";\n\nconst useApi = apiFunc => {\n  const [data, setData] = useState([]);\n  const [error, setError] = useState(false);\n  const [loading, setLoading] = useState(false);\n  const [success, setSuccess] = useState(false);\n\n  const request = async (...args) => {\n    setLoading(true);\n    setError(false);\n    setSuccess(false);\n\n    const response = await apiFunc(...args);\n    setLoading(false);\n\n    if (!response.ok) {\n      setError(true);\n    } else {\n      setData(response.data);\n      setSuccess(true);\n    }\n\n    return response;\n  };\n\n  return { data, error, loading, request, success };\n};\n\nexport default useApi;\n","/Users/mac/Desktop/React Projects/Red-Fashion/src/store.js",[],"/Users/mac/Desktop/React Projects/Red-Fashion/src/reducers/index.js",[],"/Users/mac/Desktop/React Projects/Red-Fashion/src/reducers/cartReducer.js",[],"/Users/mac/Desktop/React Projects/Red-Fashion/src/reducers/userReducer.js",[],"/Users/mac/Desktop/React Projects/Red-Fashion/src/actions/cartActions.js",[],"/Users/mac/Desktop/React Projects/Red-Fashion/src/constants/cartConstants.js",["307"],"const CART_REMOVE_ITEM = \"CART_REMOVE_ITEM\";\nconst CART_DECREASE_TOTAL_PRICE = \"CART_DECREASE_TOTAL_PRICE\";\nconst CART_INCREASE_TOTAL_PRICE = \"CART_INCREASE_TOTAL_PRICE\";\nconst CART_SET_TOTAL_PRICE = \"CART_SET_TOTAL_PRICE\";\nconst CART_SET_QUANTITY = \"CART_EDIT_QUANTITY\";\nconst CART_SET_DATA = \"CART_SET_DATA\";\nconst CART_INIT_EDITED_LIST = \"CART_INIT_EDITED_LIST\";\nconst CART_SET_EDITED_LIST = \"CART_SET_EDITED_LIST\";\n\nexport default {\n  CART_REMOVE_ITEM,\n  CART_DECREASE_TOTAL_PRICE,\n  CART_INCREASE_TOTAL_PRICE,\n  CART_SET_TOTAL_PRICE,\n  CART_SET_QUANTITY,\n  CART_SET_DATA,\n  CART_INIT_EDITED_LIST,\n  CART_SET_EDITED_LIST\n};\n","/Users/mac/Desktop/React Projects/Red-Fashion/src/constants/userConstants.js",["308"],"const USER_LOGIN = \"USER_LOGIN\";\nconst USER_LOGOUT = \"USER_LOGOUT\";\n\nexport default {\n  USER_LOGIN,\n  USER_LOGOUT\n};\n","/Users/mac/Desktop/React Projects/Red-Fashion/src/actions/userActions.js",[],"/Users/mac/Desktop/React Projects/Red-Fashion/src/auth/authStorage.js",["309"],"import jwtDecode from \"jwt-decode\";\nconst key = \"authToken\";\n\nconst storeToken = async authToken => {\n  try {\n    await localStorage.setItem(key, authToken);\n  } catch (error) {\n    console.log(\"Store Token Error\", error);\n  }\n};\n\nconst getToken = async () => {\n  try {\n    const authToken = await localStorage.getItem(key);\n    console.log(authToken);\n    return authToken;\n  } catch (error) {\n    console.log(\"Get Token Error\", error);\n  }\n};\n\nconst getUser = async () => {\n  const authToken = await getToken();\n  if (authToken) return jwtDecode(authToken);\n  return null;\n};\n\nconst removeToken = async () => {\n  try {\n    await localStorage.removeItem(key);\n  } catch (error) {\n    console.log(\"Remove Token Error\", error);\n  }\n};\n\nexport default {\n  storeToken,\n  getToken,\n  getUser,\n  removeToken\n};\n","/Users/mac/Desktop/React Projects/Red-Fashion/src/auth/useAuth.js",["310"],"import jwtDecode from \"jwt-decode\";\nimport authStorage from \"./authStorage\";\nimport { useSelector, useDispatch } from \"react-redux\";\nimport { loginAction, logoutAction } from \"../actions/userActions\";\n\nconst useAuth = () => {\n  const dispatch = useDispatch();\n  const login = authToken => {\n    // when user login succesfully => a jwt is returned\n    // => then decode the jwt to get the user's info\n    const user = jwtDecode(authToken);\n    console.log(user);\n    // store the user's info into the context for global usage\n    dispatch(loginAction(user));\n\n    // store the jwt to the storage\n    // the jwt is for calling protected api\n    authStorage.storeToken(authToken);\n  };\n\n  const logout = () => {\n    // remove the user's info from the context\n    dispatch(logoutAction());\n\n    // remove the jwt from the storage\n    authStorage.removeToken();\n  };\n\n  return { login, logout };\n};\n\nexport default useAuth;\n","/Users/mac/Desktop/React Projects/Red-Fashion/src/pages/ItemDetailPage.js",["311","312","313","314"],"import React, { useState, useEffect } from \"react\";\nimport * as itemApi from \"../APIs/itemApi\";\nimport useApi from \"../hooks/useApi\";\n\nimport ItemDetailContainer from \"../components/page/ItemDetailPage/ItemDetailContainer\";\nimport Loader from \"../components/common/Loader\";\nimport Page from \"../components/Page\";\nimport ErrorMessage from \"../components/ErrorMessage\";\nimport Container from \"../components/common/Container\";\nimport ItemCard from \"../components/card/ItemCard\";\nimport CardList from \"../components/card/CardList\";\n\nconst ItemDetailPage = ({ match }) => {\n  const id = match.params.id;\n  const getItemByIdApi = useApi(itemApi.getItemById);\n\n  useEffect(() => {\n    getItemByIdApi.request(id);\n  }, []);\n\n  return (\n    <Page>\n      <div className=\"bg-dark\">\n        <br></br>\n        <br></br>\n        <div className=\"container\">\n          {getItemByIdApi.success && getItemByIdApi.data[0] !== undefined && (\n            <>\n              {getItemByIdApi.loading && <Loader></Loader>}\n              {getItemByIdApi.error && (\n                <ErrorMessage\n                  _onClick={() => getItemByIdApi.request(id)}\n                ></ErrorMessage>\n              )}\n              {getItemByIdApi.data[0] && (\n                <>\n                  <ItemDetailContainer\n                    _item={getItemByIdApi.data[0]}\n                  ></ItemDetailContainer>\n\n                  <br></br>\n                  <br></br>\n                  <Container _hasHeader _title=\"Description\" _iconName=\"tag\">\n                    <span className=\"text-white\">\n                      {getItemByIdApi.data[0].description}\n                    </span>\n                  </Container>\n                </>\n              )}\n            </>\n          )}\n        </div>\n        <br></br>\n        <br></br>\n      </div>\n    </Page>\n  );\n};\n\nexport default ItemDetailPage;\n","/Users/mac/Desktop/React Projects/Red-Fashion/src/pages/AdminPage.js",[],"/Users/mac/Desktop/React Projects/Red-Fashion/src/pages/ItemListPage.js",["315","316"],"import React, { useEffect } from \"react\";\nimport CardList from \"../components/card/CardList\";\nimport * as itemApi from \"../APIs/itemApi\";\nimport useApi from \"../hooks/useApi\";\nimport Page from \"../components/Page\";\nimport Loader from \"../components/common/Loader\";\nimport ItemCard from \"../components/card/ItemCard\";\n\nconst ItemListPage = ({ match }) => {\n  const category = match.params.category;\n  const getItemsByCategoryApi = useApi(itemApi.getItemsByCategory);\n\n  useEffect(async () => {\n    await getItemsByCategoryApi.request(category);\n  }, [match.params.category]);\n\n  return (\n    <Page>\n      <div className=\"bg-dark\">\n        <div class=\"container\">\n          <br></br>\n          <h2 className=\"text-orange-g text-center\">LATEST ITEMS</h2>\n\n          {getItemsByCategoryApi.loading && <Loader></Loader>}\n          {getItemsByCategoryApi.data && (\n            <CardList\n              _data={getItemsByCategoryApi.data}\n              _component={ItemCard}\n            ></CardList>\n          )}\n          <br></br>\n        </div>\n      </div>\n    </Page>\n  );\n};\n\nexport default ItemListPage;\n","/Users/mac/Desktop/React Projects/Red-Fashion/src/pages/HomePage.js",["317","318","319","320","321","322","323","324","325"],"/Users/mac/Desktop/React Projects/Red-Fashion/src/pages/CartPage.js",[],"/Users/mac/Desktop/React Projects/Red-Fashion/src/components/common/Space.js",[],"/Users/mac/Desktop/React Projects/Red-Fashion/src/components/card/CardList.js",["326"],"import React from \"react\";\n\nconst CardList = ({ _data = [], _component }) => {\n  return (\n    <div className=\"card-columns\">\n      {_data.map((item, i) => {\n        return <_component _item={item} _key={i}></_component>;\n      })}\n    </div>\n  );\n};\n\nexport default CardList;\n","/Users/mac/Desktop/React Projects/Red-Fashion/src/components/modal/RegisterModal.js",["327","328"],"import React, { useState } from \"react\";\nimport Input from \"../common/Input\";\nimport Button from \"../common/Button\";\nimport Checkbox from \"../common/Checkbox\";\n\nimport $ from \"jquery\";\nimport * as authApi from \"../../APIs/authApi\";\n\nimport useApi from \"../../hooks/useApi\";\nimport useAuth from \"../../auth/useAuth\";\nimport Space from \"../common/Space\";\n\nimport styles from \"./LoginModal.module.scss\";\nimport ContainerCss from \"../common/Container.module.scss\";\nimport HeaderCss from \"../Header.module.scss\";\n\nconst LoginModal = () => {\n  const auth = useAuth();\n  const [name, setName] = useState();\n  const [email, setEmail] = useState();\n  const [address, setAddress] = useState();\n  const [password, setPassword] = useState();\n  const [policyAgree, setPoilicyAgree] = useState(false);\n\n  const registerApi = useApi(authApi.register);\n  const loginApi = useApi(authApi.login);\n\n  const registerHanling = async () => {\n    const response = await registerApi.request(email, name, address, password);\n    if (response.ok) {\n      alert(\"Register Success\");\n      loginHandling();\n    } else {\n      alert(\"Register Failed\");\n    }\n  };\n\n  const loginHandling = async () => {\n    const response = await loginApi.request(email, password);\n    if (response.ok) {\n      if (response.data.length !== 0) {\n        alert(\"Login Success\");\n        auth.login(response.data.access_token);\n        // $(\"#registerModal\").modal(\"hide\");\n        window.location.reload(false);\n      } else {\n        alert(\"Invalid name or password\");\n      }\n    } else {\n      alert(\"Login Failed\");\n    }\n  };\n\n  return (\n    <div id=\"registerModal\" className=\"modal fade overflow-auto\">\n      <div className=\"modal-dialog\">\n        <div className={`modal-content ${ContainerCss[\"body\"]}`}>\n          <div className={`modal-header ${ContainerCss[\"header\"]}`}>\n            <h4 className={ContainerCss[\"title\"]}>\n              <i className=\"fa fa-sign-in\"></i>\n              <Space></Space>\n              <Space></Space>Register\n            </h4>\n            <button\n              className={styles[\"close-btn\"]}\n              onClick={() => $(\"#registerModal\").modal(\"hide\")}\n            >\n              <i className=\"fa fa-times\"></i>\n            </button>\n          </div>\n\n          <div className=\"modal-body modal-single text-center\">\n            <h1 className={HeaderCss[\"logo\"]}>Fashion</h1>\n            <br></br>\n            <br></br>\n            <Input\n              _iconName=\"envelope\"\n              _placeholder=\"Email\"\n              _wrapperClass=\"input-1\"\n              _onChange={event => setEmail(event.target.value)}\n            ></Input>\n            <br></br>\n            <Input\n              _iconName=\"user\"\n              _placeholder=\"Name\"\n              _wrapperClass=\"input-1\"\n              _onChange={event => setName(event.target.value)}\n            ></Input>\n            <br></br>\n            <Input\n              _iconName=\"map-marker\"\n              _placeholder=\"Address\"\n              _wrapperClass=\"input-1\"\n              _onChange={event => setAddress(event.target.value)}\n            ></Input>\n            <br></br>\n            <Input\n              _iconName=\"lock\"\n              _placeholder=\"Password\"\n              _wrapperClass=\"input-1\"\n              _inputType=\"password\"\n              _onChange={event => setPassword(event.target.value)}\n            ></Input>\n            <br></br>\n            <Checkbox _className=\"text-white\">\n              Please agree to the Terms of Use & Privacy Policy\n            </Checkbox>\n            <br></br>\n            <Button\n              _onClick={() => registerHanling()}\n              _className=\"btn-yellow btn-block\"\n              _loading={registerApi.loading || loginApi.loading}\n            >\n              Register\n            </Button>\n            <br></br>\n            <span className=\"text-pink text-center\">\n              Already have an account?\n            </span>\n            <br></br>\n            <br></br>\n            <Button\n              _className=\"btn-pink btn-block\"\n              _onClick={() => {\n                $(\"#registerModal\").modal(\"hide\");\n                $(\"#loginModal\").modal(\"show\");\n              }}\n            >\n              Login\n            </Button>\n            <br></br>\n          </div>\n        </div>\n      </div>\n    </div>\n  );\n};\n\nexport default LoginModal;\n","/Users/mac/Desktop/React Projects/Red-Fashion/src/components/modal/LoginModal.js",[],"/Users/mac/Desktop/React Projects/Red-Fashion/src/components/card/ItemCard.js",["329","330"],"import React, { useEffect, useState } from \"react\";\nimport useApi from \"../../hooks/useApi\";\nimport { useHistory } from \"react-router-dom\";\nimport { useSelector } from \"react-redux\";\nimport * as cartItemApi from \"../../APIs/cartItemApi\";\nimport Button from \"../common/Button\";\nimport Input from \"../common/Input\";\nimport Space from \"../common/Space\";\nimport ItemCardCss from \"./ItemCard.module.scss\";\nimport ContainerCss from \"../common/Container.module.scss\";\nimport NumberInput from \"../common/NumberInput\";\nimport baseURL from \"../../APIs/baseURL\";\n\nconst ItemCard = ({ _item, _key }) => {\n  const user = useSelector(state => state.user.data);\n\n  const history = useHistory();\n  const getItemFromCurrentCartByItemIdApi = useApi(\n    cartItemApi.getItemFromCurrentCartByItemId\n  );\n\n  const updateItemQuantityFromCurrentCartApi = useApi(\n    cartItemApi.updateItemQuantityFromCurrentCart\n  );\n\n  const addItemIntoCurrentCartApi = useApi(cartItemApi.addItemIntoCurrentCart);\n\n  const deleteItemFromCurrentCartApi = useApi(\n    cartItemApi.deleteItemFromCurrentCart\n  );\n\n  const [quantity, setQuantity] = useState();\n  const [itemInCart, setItemInCart] = useState();\n  const [quantityEdited, setQuantityEdited] = useState(false);\n\n  const addItemIntoCurrentCartHandling = async (clientId, itemId, quantity) => {\n    const response = await addItemIntoCurrentCartApi.request(\n      clientId,\n      itemId,\n      quantity\n    );\n    if (response.ok) {\n      setItemInCart(true);\n      alert(\"addItemIntoCurrentCart Succ\");\n    } else {\n      alert(\"addItemIntoCurrentCart Failed\");\n    }\n  };\n\n  const updateItemQuantityFromCurrentHandling = async (\n    clientId,\n    itemId,\n    quantity\n  ) => {\n    const response = await updateItemQuantityFromCurrentCartApi.request(\n      clientId,\n      itemId,\n      quantity\n    );\n    if (response.ok) {\n      setQuantityEdited(false);\n    } else {\n      alert(\"updateItemQuantityFromCurrentCart Failed\");\n    }\n  };\n\n  const deleteItemFromCurrentCartHandling = async (clientId, itemId) => {\n    const response = await deleteItemFromCurrentCartApi.request(\n      clientId,\n      itemId\n    );\n    if (response.ok) {\n      setQuantityEdited(false);\n      setItemInCart(false);\n    } else {\n      alert(\"deleteItemFromCurrentCart Failed\");\n    }\n  };\n\n  const getItemFromCurrentCartByItemIdHandling = async (clientId, itemId) => {\n    const response = await getItemFromCurrentCartByItemIdApi.request(\n      clientId,\n      itemId\n    );\n    if (response.ok) {\n      if (response.data[0] !== undefined) {\n        setItemInCart(true);\n        setQuantity(response.data[0].quantity);\n      } else {\n        setItemInCart(false);\n        setQuantity(1);\n      }\n    } else {\n      alert(\"getItemFromCurrentCartByItemId Failed\");\n    }\n  };\n\n  useEffect(() => {\n    if (user != null) {\n      getItemFromCurrentCartByItemIdHandling(user.id, _item.id);\n    }\n  }, []);\n\n  return (\n    <div className={`card ${ContainerCss[\"body\"]}`} key={_key}>\n      <div className={`card-header ${ContainerCss[\"header\"]}`}>\n        <strong className=\"text-white\">{_item.item_name}</strong>\n      </div>\n\n      {/* item image */}\n      <img\n        className={`card-img-top ${ItemCardCss[\"img\"]}`}\n        src={baseURL + \"/images/store/item-\" + _item.id + \".png\"}\n        alt=\"\"\n      ></img>\n\n      {/* card body */}\n      <div className=\"card-body\">\n        <li className=\"divider-dark\">\n          <strong className=\"text-yellow\">\n            <i className=\"fa fa-black-tie\"></i>\n            <Space></Space>\n            <Space></Space>\n            Category\n          </strong>\n          <strong className=\"text-white\">{_item.category}</strong>\n        </li>\n        <li className=\"divider-dark\">\n          <strong className=\"text-yellow\">\n            <i className=\"fa fa-star\"></i>\n            <Space></Space>\n            <Space></Space>\n            Origin\n          </strong>\n          <strong className=\"text-white\">{_item.origin}</strong>\n        </li>\n        <li className=\"divider-dark\">\n          <strong className=\"text-yellow\">\n            <i className=\"fa fa-money\"></i>\n            <Space></Space>\n            <Space></Space>\n            Price\n          </strong>\n          <strong className=\"text-white\">{\"$\" + _item.price}</strong>\n        </li>\n\n        <li className=\"divider-dark\">\n          <strong className=\"text-yellow\">\n            <i className=\"fa fa-shopping-cart\"></i>\n            <Space></Space>\n            <Space></Space>\n            Cart\n          </strong>\n          {!itemInCart && (\n            <NumberInput\n              _maxLength={2}\n              _value={quantity}\n              _disabled={getItemFromCurrentCartByItemIdApi.loading}\n              _onChange={event => setQuantity(event.target.value)}\n              _onClickMinus={() => setQuantity(quantity - 1)}\n              _onClickPlus={() => setQuantity(quantity + 1)}\n            ></NumberInput>\n          )}\n          {itemInCart && (\n            <NumberInput\n              _maxLength={2}\n              _value={quantity}\n              _disabled={getItemFromCurrentCartByItemIdApi.loading}\n              _onChange={event => {\n                setQuantity(event.target.value);\n                setQuantityEdited(true);\n              }}\n              _onClickMinus={() => {\n                setQuantity(parseInt(quantity) - 1);\n                setQuantityEdited(true);\n              }}\n              _onClickPlus={() => {\n                setQuantity(parseInt(quantity) + 1);\n                setQuantityEdited(true);\n              }}\n            ></NumberInput>\n          )}\n        </li>\n\n        {/* item detail btn */}\n        <br></br>\n        <Button\n          _className=\"btn-green btn-block\"\n          _iconName=\"tags\"\n          _onClick={() => history.push(\"/item_detail/\" + _item.id)}\n        >\n          Item Detail\n        </Button>\n\n        {/* CRUD buttons */}\n        {getItemFromCurrentCartByItemIdApi.loading && (\n          <>\n            <br></br>\n            <Button\n              _className=\"btn-pink btn-block\"\n              _iconName=\"tags\"\n              _loading={getItemFromCurrentCartByItemIdApi.loading}\n            ></Button>\n          </>\n        )}\n        {getItemFromCurrentCartByItemIdApi.success && (\n          <>\n            {/* add item btn */}\n            {user !== null && !itemInCart && (\n              <>\n                <br></br>\n                <Button\n                  _loading={addItemIntoCurrentCartApi.loading}\n                  _onClick={() =>\n                    addItemIntoCurrentCartHandling(user.id, _item.id, quantity)\n                  }\n                  _iconName=\"cart-plus\"\n                  _className=\"btn-yellow btn-block\"\n                >\n                  Add to Cart\n                </Button>\n              </>\n            )}\n\n            {/* update btn */}\n            {itemInCart && quantityEdited && (\n              <>\n                <br></br>\n                <Button\n                  _loading={updateItemQuantityFromCurrentCartApi.loading}\n                  _onClick={() =>\n                    updateItemQuantityFromCurrentHandling(\n                      user.id,\n                      _item.id,\n                      quantity\n                    )\n                  }\n                  _iconName=\"pencil\"\n                  _className=\"btn-yellow btn-block\"\n                >\n                  Update\n                </Button>\n              </>\n            )}\n\n            {/* remove item btn */}\n            {user !== null && itemInCart && (\n              <>\n                <br></br>\n                <Button\n                  _loading={deleteItemFromCurrentCartApi.loading}\n                  _onClick={() =>\n                    deleteItemFromCurrentCartHandling(user.id, _item.id)\n                  }\n                  _className=\"btn-pink btn-block\"\n                  _iconName=\"cart-arrow-down\"\n                >\n                  Remove from Cart\n                </Button>\n              </>\n            )}\n          </>\n        )}\n\n        {/* no user logged in */}\n        {user === null && (\n          <div className=\"text-center\">\n            <br></br>\n            <strong className={\"text-pink\"}>\n              You have not login to save a cart\n            </strong>\n            <br></br>\n            <br></br>\n            <Button _className=\"btn-pink btn-block\" _iconName=\"sign-in\">\n              Login\n            </Button>\n          </div>\n        )}\n      </div>\n    </div>\n  );\n};\n\nexport default ItemCard;\n","/Users/mac/Desktop/React Projects/Red-Fashion/src/hooks/useLink.js",[],"/Users/mac/Desktop/React Projects/Red-Fashion/src/components/common/Button.js",["331"],"import React from \"react\";\nimport Loader from \"./Loader\";\nimport Space from \"./Space\";\nimport colors from \"../../configs/colors\";\n\nconst Button = ({\n  children,\n  _onClick,\n  _loading,\n  _disabled,\n  _hidden,\n  _iconName,\n  _className = \"btn btn-info btn-block\"\n}) => {\n  return (\n    <>\n      {!_loading && (\n        <button\n          className={_className}\n          disabled={_disabled}\n          hidden={_hidden}\n          onClick={_onClick}\n        >\n          {_iconName && <i className={\"fa fa-\" + _iconName}></i>}\n          {_iconName && children && (\n            <>\n              <Space></Space>\n              <Space></Space>\n            </>\n          )}\n          {children}\n        </button>\n      )}\n      {_loading && (\n        <button className={_className} disabled={true}>\n          <Loader _height={15}></Loader>\n        </button>\n      )}\n    </>\n  );\n};\n\nexport default Button;\n","/Users/mac/Desktop/React Projects/Red-Fashion/src/components/common/Loader.js",[],"/Users/mac/Des