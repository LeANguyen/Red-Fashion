 => {\n  const [items, setItems] = useState([]);\n\n  const getItemsApi = useApi(itemApi.getItems);\n\n  const getItemsHandling = async (skip, limit) => {\n    const newItems = [...items];\n    const response = await getItemsApi.request(skip, limit);\n    if (response.ok) {\n      setItems(newItems.concat(response.data));\n    }\n  };\n\n  useEffect(() => {\n    getItemsHandling(items.length, 3);\n  }, []);\n\n  return (\n    <div>\n      <h2 className=\"text-yellow-g text-center\">LATEST ITEMS</h2>\n      <br></br>\n      {getItemsApi.loading && <Loader></Loader>}\n      {getItemsApi.success && items.length !== 0 && (\n        <>\n          <CardList _data={items} _component={ItemCard}></CardList>\n          <br></br>\n          <Button\n            _onClick={() => {\n              getItemsHandling(items.length, 1);\n            }}\n            _block\n            _className={`btn-yellow btn-block`}\n            _loading={getItemsApi.loading}\n          >\n            Load More\n          </Button>\n        </>\n      )}\n    </div>\n  );\n};\n\nexport default ItemCardList;\n","/Users/mac/Desktop/React Projects/Red-Fashion/src/components/page/HomePage/AboutUs.js",[],"/Users/mac/Desktop/React Projects/Red-Fashion/src/configs/colors.js",[],"/Users/mac/Desktop/React Projects/Red-Fashion/src/configs/settings.js",[],["338","339"],"/Users/mac/Desktop/React Projects/Red-Fashion/src/APIs/cartItemApi.js",[],"/Users/mac/Desktop/React Projects/Red-Fashion/src/APIs/itemApi.js",[],"/Users/mac/Desktop/React Projects/Red-Fashion/src/APIs/baseURL.js",[],"/Users/mac/Desktop/React Projects/Red-Fashion/src/APIs/authApi.js",[],"/Users/mac/Desktop/React Projects/Red-Fashion/src/APIs/cartApi.js",[],"/Users/mac/Desktop/React Projects/Red-Fashion/src/APIs/client.js",["340"],"import { create } from \"apisauce\";\nimport authStorage from \"../auth/authStorage\";\nimport baseURL from \"./baseURL\";\n\nconst api = create({\n  baseURL: baseURL,\n  headers: {\n    \"Content-Type\": \"applicatio