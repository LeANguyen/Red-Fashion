t localStorage.setItem(key, authToken);\n  } catch (error) {\n    console.log(\"Store Token Error\", error);\n  }\n};\n\nconst getToken = async () => {\n  try {\n    const authToken = await localStorage.getItem(key);\n    console.log(authToken);\n    return authToken;\n  } catch (error) {\n    console.log(\"Get Token Error\", error);\n  }\n};\n\nconst getUser = async () => {\n  const authToken = await getToken();\n  if (authToken) return jwtDecode(authToken);\n  return null;\n};\n\nconst removeToken = async () => {\n  try {\n    await localStorage.removeItem(key);\n  } catch (error) {\n    console.log(\"Remove Token Error\", error);\n  }\n};\n\nexport default {\n  storeToken,\n  getToken,\n  getUser,\n  removeToken\n};\n","/Users/mac/Desktop/React Projects/Red-Fashion/src/auth/useAuth.js",["300"],"import jwtDecode from \"jwt-decode\";\nimport authStorage from \"./authStorage\";\nimport { useSelector, useDispatch } from \"react-redux\";\nimport { loginAction, logoutAction } from \"../actions/userActions\";\n\nconst useAuth = () => {\n  const dispatch = useDispatch();\n  const login = authToken => {\n    // when user login succesfully => a jwt is returned\n    // => then decode the jwt to get the user's info\n    const user = jwtDecode(authToken);\n    console.log(user);\n    // store the user's info into the context for global usage\n    dispatch(loginAction(user));\n\n    // store the jwt to the storage\n    // the jwt is for calling protected api\n    authStorage.storeToken(authToken);\n  };\n\n  const logout = () => {\n    // remove the user's info from the context\n    dispatch(logoutAction());\n\n    // remove the jwt from the storage\n    authStorage.removeToken();\n  };\n\n  return { login, logout };\n};\n\nexport default useAuth;\n","/Users/mac/Desktop/React Projects/Red-Fashion/src/pages/ItemDetailPage.js",["301","302","303","304"],"import React, { useState, useEffect } from \"react\";\nimport * as itemApi from \"../APIs/itemApi\";\nimport useApi from \"../hooks/useApi\";\n\nimport ItemDetailContainer from \"../components/page/ItemDetailPage/ItemDetailContainer\";\nimport Loader from \"../components/common/Loader\";\nimport Page from \"../components/Page\";\nimport ErrorMessage from \"../components/ErrorMessage\";\nimport Container from \"../components/common/Container\";\nimport ItemCard from \"../components/card/ItemCard\";\nimport CardList from \"../components/card/CardList\";\n\nconst ItemDetailPage = ({ match }) => {\n  const id = match.params.id;\n  const getItemByIdApi = useApi(itemApi.getItemById);\n\n  useEffect(() => {\n    getItemByIdApi.request(id);\n  }, []);\n\n  return (\n    <Page>\n      <div className=\"bg-dark\">\n        <br></br>\n        <br></br>\n        <div className=\"container\">\n          {getItemByIdApi.success && getItemByIdApi.data[0] !== undefined && (\n            <>\n              {getItemByIdApi.loading && <Loader></Loader>}\n              {getItemByIdApi.error && (\n                <ErrorMessage\n                  _onClick={() => getItemByIdApi.request(id)}\n                ></ErrorMessage>\n              )}\n              {getItemByIdApi.data[0] && (\n                <>\n                  <ItemDetailContainer\n                    _item={getItemByIdApi.data[0]}\n                  ></ItemDetailContainer>\n\n                  <br></br>\n                  <br></br>\n                  <Container _hasHeader _title=\"Description\" _iconName=\"tag\">\n                    <span className=\"text-white\">\n                      {getItemByIdApi.data[0].description}\n                    </span>\n                  </Container>\n                </>\n              )}\n            </>\n          )}\n        </div>\n        <br></br>\n        <br></br>\n      </div>\n    </Page>\n  );\n};\n\nexport default ItemDetailPage;\n","/Users/mac/Desktop/React Projects/Red-Fashion/src/pages/AdminPage.js",[],"/Users/mac/Desktop/React Projects/Red-Fashion/src/pages/ItemListPage.js",["305","306"],"import React,